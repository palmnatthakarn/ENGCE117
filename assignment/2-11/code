#include<stdio.h>
int main(){
    
	int y = 9 ;
	int x;
	int z = 9;
	int a = 9;
	int i;
	
    printf("User Input : ");
    scanf("%d" , &x); 

	printf("----\n");
	printf("Series =");
	printf(" 9");
	
	for(i = 1 ; i < x ; i++)
	{
        y = y * 10;
        z = z + y;
        a = z + a;
        
		printf("+%d",z);   
    }
		printf("\n");
		printf("Sum = %d",a);
}
     
